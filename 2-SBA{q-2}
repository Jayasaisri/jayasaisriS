package com.mupack.colldemo;
import java.util.*;
public class ReverseBreak{
String reversebreak(String reversebreak){
char[ ] arr=reversebreak.toCharArray();
String rev="";
for(int i=arr.length-1; i>= 0; i--){
arr[i]=arr[i];
rev=rev+arr[i];
}
String s=rev;
StringTokenizer str1=new StringTokenizer(s," ");
String srev="";
while(str1.hasMoreTokens()){
srev=str1.nextToken()+System.lineSeperator()+srev;
}
System.out.println(srev);
return " ";
public static void main(String[] args){
ReverseBreak rev=new ReverseBreak();
String rev1= rev.reversebreak("This Is Java Programming");
System.out.println(rev1);
}
}
